<main class="description">
  <table>
    <thead>
      <tr>
        <th colspan="3">Apartment &num; <%= @apartment.apartment_number %></th>
      </tr>
    </thead>
    <tbody>
      <% @rents.each do |rent|%>
        <tr>
          <td><%= rent.amount %></td>
          <td><%= rent.month %></td>
          <td><%= rent.pay_status %></td>
        </tr>
      <%end%>
    </tbody>
  </table>
</main>